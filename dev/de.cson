  elements: [
    { type: 'square-rounded', color: 'black' }
    { type: 'square-rounded', color: 'white', transform: 'scale(.95)' }
    { type: 'circle-bg', color: 'red', transform: 'translate(0,-8%) scale(.7)' }
    { type: 'circle-bg', color: 'white', transform: 'translate(0,-8%) scale(.7) {fit_border_circle}' }
    { type: 'content-4', content: 'Umwelt', color: 'black', transform: 'translate(0,-50%) scale(.8) {fit_border_circle}' }
    { type: 'content-4', content: 'ZONE', color: 'black', transform: 'translate(0,150%) scale(.8)'}
  ]

prohibitory_low_emission_zone_end:
  category: 'prohibitory'
